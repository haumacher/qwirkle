<?xml version="1.0" encoding="UTF-8"?>
<!--
  When updating your version of GWT, you should also update this DTD reference,
  so that your app can take advantage of the latest GWT module capabilities.
-->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.9.0//EN"
  "http://www.gwtproject.org/doctype/2.9.0/gwt-module.dtd">
<module rename-to="qwirkle">
  <inherits name='de.haumacher.msgbuf.MsgBuf'/>
  <inherits name='qwirkle.common.Common'/>

  <inherits name="org.dominokit.domino.ui.DominoUI" />

  <inherits name='elemental2.dom.Dom'/>
  <inherits name='elemental2.core.Core'/>
  <inherits name='elemental2.svg.Svg'/>

  <!-- Inherit the core Web Toolkit stuff. -->
  <inherits name="com.google.gwt.user.User"/>
  
  <entry-point class='qwirkle.app.client.App'/>

  <source path='client'/>
  <source path='shared'/>

  <!-- allow Super Dev Mode -->
  <add-linker name="xsiframe"/>
</module>
